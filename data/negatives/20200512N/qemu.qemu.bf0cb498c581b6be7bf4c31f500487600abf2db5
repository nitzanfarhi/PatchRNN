 {
     VirtIODevice vdev;
-    VirtQueue *ivq, *dvq;
+    VirtQueue *ivq, *ovq;
     CharDriverState *chr;
 } VirtIOConsole;
 
@@ -132,7 +132,7 @@ VirtIODevice *virtio_console_init(DeviceState *dev)
