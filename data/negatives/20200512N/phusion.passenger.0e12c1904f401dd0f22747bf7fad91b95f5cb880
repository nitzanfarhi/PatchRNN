+	bool operator==(const DataStoreId &other) const {
+		if (id == NULL) {
+			return other.id == NULL;
+		} else {
+			if (other.id == NULL) {
+				return false;
+			} else {
+				return toStaticString() == other.toStaticString();
+			}
+		}
+	}
