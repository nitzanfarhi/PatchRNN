+++ b/drivers/staging/wilc1000/coreconfigurator.h
@@ -109,7 +109,7 @@ struct connect_info {
 	u8 bssid[6];
 	u8 *req_ies;
 	size_t req_ies_len;
-	u8 *pu8RespIEs;
+	u8 *resp_ies;
 	u16 u16RespIEsLen;
 	u16 u16ConnectStatus;
 };
