+    ObjDesc->Common.Size            = sizeof (ACPI_OBJECT_INTERNAL);
+    ObjDesc->Common.Flags           = AO_STATIC_ALLOCATION;
+    ObjDesc->Common.ReferenceCount  = 1;
 
     return_VOID;
