 static UINT disp_plugin_terminated(IWTSPlugin* pPlugin)
 {
+	DISP_PLUGIN* disp = (DISP_PLUGIN*) pPlugin;
+	free(disp->listener_callback);
+	free(disp->iface.pInterface);
 	free(pPlugin);
 	return CHANNEL_RC_OK;
 }

